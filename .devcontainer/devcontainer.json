{
    "name": "Password Manager",
    "image": "mcr.microsoft.com/devcontainers/javascript-node:0-14",
    "postCreateCommand": "npm install",
    "customizations": {
      "vscode": {
        "extensions": [
          "dbaeumer.vscode-eslint",
          "esbenp.prettier-vscode"
        ]
      }
    },
    "forwardPorts": [3000, 5000],
    "features": {
      "postgresql": {
        "version": "13"
      }
    }
  }
  
